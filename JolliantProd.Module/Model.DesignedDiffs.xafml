  <ActionDesign>
    <Actions>
      <Action Id="ApprovePOAction" TargetObjectsCriteria="[Company] Is Not Null And [Vendor] Is Not Null And [DeliveryLocation] Is Not Null And [Status] = ##Enum#JolliantProd.Module.BusinessObjects.PurchaseOrder+StatusEnum,New# And IsCurrentUserInRole('Purchase Manager')" />
